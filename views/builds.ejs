<html>
	<head>
		<title>Completed Builds</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
		<link rel="stylesheet" href="assets/css/profile.css">
		<link rel="stylesheet" href="assets/css/build.css">
		<link rel="stylesheet" href="assets/css/build2.css">
		<script src="https://kit.fontawesome.com/f9b37ef16f.js" crossorigin="anonymous"></script>
	</head>
	<body class="homepage is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="wholeHeader">
					<div id="header" class="container">

						<!-- Logo -->
							<h1 id="logo"><a href="/profile"> <img src="img/ab.png" alt=""> </a></h1>

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li><a href="/forums">Draft Builds</a></li>
									<li><a href="/builds">Completed Builds</a></li>
									<li class="break"><a href="/profile">Profile</a></li>
									<li><a href="/login"><i class="fas fa-sign-in-alt"></i></a></li>

								</ul>
							</nav>

					</div>

					<!-- Hero -->
						<section id="hero" class="container">
							<header>
								<h2>Completed Builds</h2>
							</header>
							<p>Show off your computer specs and setup here!</p>
							<p>View other people's setups as well!</p>
					
							</ul> -->
						</section>

				</div>
		
				<div class="wrapper">
					<div class="container">
						<div id="profileFirstId" class="row">
							<section id="profileSection">
								<h1><%= typeof msg != 'undefined' ? msg : '' %></h1>
								<form action="/submissionPost" method="POST" enctype="multipart/form-data">
									<div class="text-input">
										<input name='caption' type="text" id="input1" placeholder="            Cost">
										<label for="input1"><%=userName%></label>
										<input type="text" name="motherboard" placeholder="Mothebaord">
										<input type="text" name="CPU" placeholder="Processor">
										<input type="text" name="GPU" placeholder="Graphics Card">
										<input type="text" name="RAM" placeholder="RAM">
										<input type="text" name="storage" placeholder="Storage">
										<input type="text" name="case" placeholder="Case">
										<input type="text" name="powerSupply" placeholder="Powersupply">

									  </div>
									<input class='input' type="file" name="myFile">
									
									<input class="submit" type="submit" value="Submit">
								</form>
							</section>
							<section id="profileFeed">
								<% for(var i=0; i<buildPost.length; i++) {%>
								<section id="postSection">
									<div class= "postDiv">
										<p name="userName"> <%=buildPost[i].post.userName%></p>
										
										<a href="/post?id=<%=buildPost[i].post._id%>"><img src="uploads/<%=buildPost[i].post.userIMG.filename%>" ></a>
										  
										<i class="fas fa-heart" data='<%=buildPost[i].post.likes%>'><span> <%=buildPost[i].post.likes%></span></i>
									</div>
									<div class="postDiv">
										<div>
											<h2 name="caption">$<%=buildPost[i].post.caption%></h2>
											<p>Motherboard: <%=buildPost[i].post.mobo%></p>
											<p>CPU: <%=buildPost[i].post.cpu%></p>
											<p>GPU: <%=buildPost[i].post.gpu%></p>
											<p>RAM: <%=buildPost[i].post.ram%></p>
											<p>Storage: <%=buildPost[i].post.storage%></p>
											<p>Case: <%=buildPost[i].post.case%></p>
											<p>PowerSupply: <%=buildPost[i].post.powerSupply%></p>
										</div>
										<div class="commentSection">
											<!-- <button class="collapsible">Comments</button -->
											<i class="fas fa-comments collapsible"></i>
											<div id="commentSection" class="content">
												<% for(var j=0; j<buildPost[i].comments.length; j++) {%>
													<p><%=buildPost[i].comments[j].commentName%>-<%=buildPost[i].comments[j].comment%></p>
												<% } %>
											</div>
											<form action="/commentPost/<%=buildPost[i].post._id%>" method="POST">
												<label for="inp" class="inp">
													<input class="comment" type="text" id="inp" name="comment" placeholder="&nbsp;">
													<span class="label">Comment</span>
													<span class="focus-bg"></span>
												  </label>
												<input class="submit" type="submit">
											</form>
										</div>

									</div>
								</section>
								<% } %>
								

							</section>
					</div>
				</div>

			<!-- Promo -->
				<div id="promo-wrapper">
					<section id="promo">
						<h2>Neque semper magna et lorem ipsum adipiscing</h2>
						<a href="#" class="button">Breach the thresholds</a>
					</section>
				</div>

			

			<!-- Footer -->
			<div id="footer-wrapper">
				<div id="footer" class="container">
					<header class="major">
						<h2>Leave me a message! </h2>
						<p>Intrested to know more? I'd love to talk more about computers or how I built this website. </p>
					</header>
					<div class="row">
						<section class="col-6 col-12-narrower">
							<form method="post" action="#">
								<div class="row gtr-50">
									<div class="col-6 col-12-mobile">
										<input name="name" placeholder="Name" type="text" />
									</div>
									<div class="col-6 col-12-mobile">
										<input name="email" placeholder="Email" type="text" />
									</div>
									<div class="col-12">
										<textarea name="message" placeholder="Message"></textarea>
									</div>
									<div class="col-12">
										<ul class="actions">
											<li><input type="submit" value="Send Message" /></li>
											<li><input type="reset" value="Clear form" /></li>
										</ul>
									</div>
								</div>
							</form>
						</section>
						
					</div>
				</div>
				<div id="copyright" class="container">
					<ul class="menu">
						<li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>
				</div>
			</div>

		</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.dropotron.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>
			<script
			src="https://code.jquery.com/jquery-3.2.1.min.js"
			integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
			crossorigin="anonymous"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>  
			<script src="main.js"></script>
			<script src="build.js"></script>
			
	</body>
</html>
